	title = "Hit Delay",
	value = 0.5,
	minimum_value = 0.1,
	maximum_value = 1.0,
	round_value = 0.1,
	flag = "Killaura_Delay",
	callback = function(v)
		hitCooldown = v
		print("–ó–∞–¥–µ—Ä–∂–∫–∞ –º–µ–∂–¥—É —É–¥–∞—Ä–∞–º–∏:", v)
	end
})


